{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "module": "CommonJS",
    "lib": ["ESNext", "scripthost"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["vite/client"],
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "baseUrl": "./",
    "paths": {
      "common": ["../common/src"]
    }
  },
  "include": ["src/**/*"]
  // "references": [
  //   {
  //     "path": "../common/tsconfig.json"
  //   }
  // ]
}
